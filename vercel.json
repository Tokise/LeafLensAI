{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "camera=(self), microphone=(self), geolocation=(self)"
        },
        {
          "key": "Feature-Policy",
          "value": "camera 'self'; microphone 'self'; geolocation 'self'"
        }
      ]
    }
  ]
}
